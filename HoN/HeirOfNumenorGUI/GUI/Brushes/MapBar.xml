  <Brush Name="MapTextBrushWithAnim" Font="Galahad" TextHorizontalAlignment="Center">
    <Layers>
      <BrushLayer Name="Default" />
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#F4E1C4FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" AnimationToPlayOnBegin="ValueChangeAnimation" FontSize="21" />
      <Style Name="Negative" AnimationToPlayOnBegin="NegativeValueChangeAnimation" />
      <Style Name="Positive" AnimationToPlayOnBegin="PositiveValueChangeAnimation" />
    </Styles>
    <Animations>
      <Animation Name="PositiveValueChangeAnimation" Duration="0.3" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.01" Value="#A8D521FF" />
          <KeyFrame Time="0.15" Value="#A8D521FF" />
        </AnimationProperty>
      </Animation>
      <Animation Name="NegativeValueChangeAnimation" Duration="0.3" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.01" Value="#FF0000FF" />
          <KeyFrame Time="0.15" Value="#FF0000FF" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>

  <Brush Name="MapUnreadBrushWithAnim">
    <Layers>
