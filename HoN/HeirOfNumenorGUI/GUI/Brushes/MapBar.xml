    </Layers>
    <Styles>
      <Style Name="Default" AnimationToPlayOnBegin="DefaultAnimation" />
    </Styles>
    <Animations>
      <Animation Name="DefaultAnimation" Duration="0.75" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0.0" Value="1.0" />
          <KeyFrame Time="0.1" Value="1.1" />
          <KeyFrame Time="0.2" Value="1.7" />
          <KeyFrame Time="0.375" Value="1.7" />
          <KeyFrame Time="0.55" Value="1.7" />
          <KeyFrame Time="0.65" Value="1.1" />
          <KeyFrame Time="0.75" Value="1.0" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>

